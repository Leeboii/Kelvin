local OrionLib = loadstring(game:HttpGet(('https://raw.githubusercontent.com/shlexware/Orion/main/source')))()
local Window = OrionLib:MakeWindow({Name = "Kelvin 1.0", HidePremium = false, SaveConfig = true, ConfigFolder = "OrionTest"})

local localplayer = Window:MakeTab({
	Name = "LocalPlayer",
	PremiumOnly = false
})

localplayer:AddSlider({
	Name = "WalkSpeed",
	Min = 16,
	Max = 1715,
	Default = 5,
	Color = Color3.fromRGB(255,255,255),
	Increment = 1,
	ValueName = "WalkSpeed",
	Callback = function(Value)
		 game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = Value
	end    
})

localplayer:AddButton({
	Name = "Reset",
	Callback = function()
      		game.Players.LocalPlayer.Character.Humanoid.Health = Value
  	end    
})

local hub = Window:MakeTab({
	Name = "Script Hub",
	PremiumOnly = false
})

hub:AddButton({
	Name = "DarkHub",
	Callback = function()
      		loadstring(game:HttpGet("https://raw.githubusercontent.com/RandomAdamYT/DarkHub/master/Init", true))()
  	end    
})

hub:AddButton({
	Name = "OwlHub",
	Callback = function()
      		loadstring(game:HttpGet("https://raw.githubusercontent.com/CriShoux/OwlHub/master/OwlHub.txt"))();
  	end    
})

hub:AddButton({
	Name = "Infinite Yield",
	Callback = function()
      		loadstring(game:HttpGet("https://raw.githubusercontent.com/EdgeIY/infiniteyield/master/source"))();
  	end    
})


OrionLib:MakeNotification({
	Name = "Hello!",
	Content = "Welcome to Kelvin 1.0!",
	Time = 5
})

OrionLib:Init()
